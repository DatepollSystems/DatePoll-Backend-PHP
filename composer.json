{
    "name": "datepoll/datepoll",
    "description": "DatePoll is a community management application.",
    "keywords": ["clubs", "management", "polls", "emails"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "firebase/php-jwt": "^5.3",
        "fruitcake/laravel-cors": "^2.0",
        "illuminate/mail": "^8.0",
        "illuminate/redis": "^8.0",
        "jsvrcek/ics": "^0.8.0",
        "laravel/lumen-framework": "^8.0",
        "neitanod/forceutf8": "^2.0",
        "php-imap/php-imap": "^4.1",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "league/flysystem": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "fakerphp/faker": "~1.14",
        "friendsofphp/php-cs-fixer": "^2.19",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.4",
        "nunomaduro/larastan": "^0.6.4",
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "database/migrations"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
